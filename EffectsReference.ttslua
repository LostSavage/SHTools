Effects Reference


--]]


------------------Constants
START_POS = {x = 89, y = 1.1, z = -80}


------------------Functions
function onLoad()
    local global_name = Global.getVar('mod_name')
    if global_name == 'Secret Hitler: CE' then
        self.setPositionSmooth(START_POS)
        self.setLock(true)
    end
end
